@using Borg.Cms.Basic.Lib.Features.Navigation.Modules
@using Borg.Infra.DTO
@using Borg.MVC
@using Microsoft.AspNetCore.Mvc.Razor
@{
    
}
@foreach (var s in Orchestrator.Device.Sections)
{



    DefineSection(s.Identifier, async _ =>
    {


        
        var value = await Html.RenderViewSection(Component, s);
 
        
        await Output.WriteAsync(value.ToString());
    });

}
