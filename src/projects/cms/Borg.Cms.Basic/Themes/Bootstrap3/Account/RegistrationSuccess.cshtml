@using Borg.Cms.Basic.Lib.Features.Auth.Register
@model Borg.Cms.Basic.Lib.Features.Auth.Register.RegisterViewModel


@{
    Orchestrator.Page.SetTitle("Successful registration");
}

    <div class="login-box">
        <div class="login-logo">
            <a asp-controller="Home" asp-action="Home"><img src="images/Futura-Logo-RGB-&-Blue.png" alt="futura performance unlocked" class="img-responsive image" /></a>
        </div>
        <div class="login-box-body">
            <p class="login-box-msg">
                Registration successful for <strong><span>@Model.FirstName</span> <span>@Model.LastName</span> </strong>.
                Your account will be reviewed and you will be notified.
                Thank you.
            </p>
        </div>

    </div>