@using Timesheets.Web.Features.Register
@model Timesheets.Web.Features.Register.RegisterViewModel
@*@{
    Layout = "_EmptyLayout";
   
}*@

<div class="row">
    <div class="col-md-12">


        <img src="https://d3kdj0p3ajn4xa.cloudfront.net/assets/resources/timesheets/timesheets-922dedd704c0ffddeacf702971ce82bb4c1c07b65d63534ed7790bb621fa5a10.png" class="img-responsive center-block margin-bottom" alt="logo" style="max-width:150px; margin-top:25px;" />

    </div>
    <div class="col-md-4 col-md-offset-4">

        <div class="box">
          
                <div class="box-header">
                    <h3 class="box-title">Successful Registration</h3>
                </div>
                <div class="box-body">

                    <p>
                        Registration successful for <strong><span>@Model.FirstName</span> <span>@Model.LastName</span> </strong>.
                    </p>
                    <p>Your account will be reviewed and you will be notified.</p>
                    <p>Thank you.</p>

                </div>

 
        </div>
    </div>
</div>