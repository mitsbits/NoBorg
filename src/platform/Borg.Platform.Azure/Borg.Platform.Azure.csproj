    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.0.1-beta</Version>
    <Authors>mitsbits@gmail.com</Authors>
    <Company>mitsbits@gmail.com</Company>
    <Description>Borg package for integrating with Azure services</Description>
    <Copyright>mitsbits@gmail.com</Copyright>
    <PackageProjectUrl>https://github.com/mitsbits/Borg.Infra</PackageProjectUrl>
